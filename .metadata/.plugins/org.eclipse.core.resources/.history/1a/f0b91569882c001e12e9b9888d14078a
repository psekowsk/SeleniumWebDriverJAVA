package Section07;

public class L14_Inheritance {
	Car carObject = new Car();
	carObject.rev();
	System.out.println(carObject.getMake() + " " + carObject.getModel());
}

class Vehicle {
	private String make = "BMW";
	public String getMake() {
		return make;
	}
	
	public void rev( ) {
		System.out.println("Vrooom!");
	}
}
